{
  "name": "phobos-server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "lint": "eslint '*/**/*.{js,ts}' --quiet --fix",
    "start": "DB='./dev.sqlite' ts-node src/server.ts",
    "wstart": "set DB=./dev.sqlite && ts-node src/server.ts",
    "dev": "DB='./dev.sqlite' nodemon src/server.ts",
    "wdev": "set DB=./dev.sqlite && nodemon src/server.ts",
    "dbinit": "DB='./dev.sqlite' ts-node src/scripts/dbinit.ts",
    "wdbinit": "set DB=./dev.sqlite && ts-node src/scripts/dbinit.ts",
    "dbclear": "DB='./dev.sqlite' ts-node src/scripts/dbclear.ts",
    "wdbclear": "set DB=./dev.sqlite && ts-node src/scripts/dbclear.ts"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cors": "^2.8.5",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "natural": "^5.0.5",
    "node-fetch": "^2.6.1",
    "routing-controllers": "^0.9.0",
    "routing-controllers-openapi": "^3.1.0",
    "sqlite3": "^5.0.2",
    "swagger-ui-express": "^4.1.6",
    "ts-node": "^10.2.1",
    "typedi": "^0.10.0",
    "typeorm": "^0.2.37",
    "typeorm-typedi-extensions": "^0.4.1",
    "vector-object": "^1.3.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/mailgen": "^2.0.5",
    "@types/morgan": "^1.9.3",
    "@types/natural": "^5.0.0",
    "@types/node": "^16.7.4",
    "@types/node-fetch": "^2.5.12",
    "@types/nodemailer": "^6.4.4",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "typescript": "^4.4.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint '*/**/*.{js,ts}' --quiet --fix"
    ]
  }
}
